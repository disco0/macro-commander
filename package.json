{
    "name": "sync-macros",
    "displayName": "sync-macros",
    "description": "automate repetitive actions with custom macros",
    "version": "1.0.0",
    "publisher": "jeff-hykin",
    "repository": "https://github.com/jeff-hykin/sync-macros",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "Macros configuration",
            "properties": {
                "macros": {}
            }
        },
        "commands": [
            {
                "command": "macro.run",
                "title": "Run A Macro"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "vscode": "^1.0.0"
    },
    "dependencies": {}
}